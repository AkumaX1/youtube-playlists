    "lint:fix": "standard --fix \"src/**/*.js\" && stylelint --fix 'src/**/styled.js' "
  },
  "dependencies": {
    "lodash.debounce": "^4.0.8",
    "react": "^16.13.1",
    "react-dnd": "^10.0.2",
    "react-dnd-html5-backend": "^10.0.2",
    "react-dom": "^16.13.1",
    "react-ga": "^2.7.0",
    "react-joyride": "^2.2.1",
    "react-scripts": "3.1.2",
    "react-toastify": "^5.5.0",
    "react-youtube": "^7.11.1",
    "styled-components": "^4.4.1",
    "styled-media-query": "^2.1.2",
    "undux": "^5.2.0"
  },
  "devDependencies": {
    "cypress": "^5.0.0",
    "husky": "^4.2.3",
    "lint-staged": "^10.1.2",
    "standard": "^14.3.3",
    "stylelint": "^13.3.0",
    "stylelint-config-recommended": "^3.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.10.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "standard": {
    "parser": "babel-eslint",
    "ignore": [
      "cypress/**/*.js"
    ]
  }
