{
  "name": "youtube-playlists",
  "description": "Create on-the-fly YouTube playlists",
  "version": "3.2.2",
  "author": "ndom91 <yo@ndo.dev>",
  "homepage": "https://youtube.ndo.dev",
  "repository": "https://github.com/ndom91/youtube-playlists",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ndom91/youtube-playlists/issues"
  },
  "scripts": {
    "start": "BROWSER=none react-scripts start",
    "build": "react-scripts build",
    "test": "cypress open",
    "eject": "react-scripts eject",
    "deploy": "gh-pages -d build",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "lint:js": "standard \"src/**/*.js\"",
    "lint:css": "stylelint './src/**/styled.js'",
    "prettier": "prettier --check \"src/**/*.js\"",
    "prettier:fix": "prettier --write \"src/**/*.js\" && stylelint --fix 'src/**/styled.js' "
  },
  "dependencies": {
    "lodash.debounce": "^4.0.8",
    "react": "^16.13.1",
    "react-dnd": "^11.1.3",
    "react-dnd-html5-backend": "^11.1.3",
    "react-dom": "^16.13.1",
    "react-ga": "^3.1.2",
    "react-joyride": "^2.2.1",
    "react-scripts": "3.4.3",
    "react-toastify": "^6.0.8",
    "react-youtube": "^7.12.0",
    "styled-components": "^5.1.1",
    "styled-media-query": "^2.1.2",
    "undux": "^5.2.0"
  },
  "devDependencies": {
    "cypress": "^5.0.0",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.12",
    "prettier": "^2.1.0",
    "stylelint": "^13.6.1",
    "stylelint-config-recommended": "^3.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.10.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "standard": {
    "parser": "babel-eslint",
    "ignore": [
      "cypress/**/*.js"
    ]
  }
}
